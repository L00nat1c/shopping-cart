<%- include('./partials/head') %>
    <script src="/product-list.js" defer></script>
    </head>

    <body>

        <%- include('.partials/nav') %>

            <h1>
                <%= title %>
            </h1>
            <div id="root"> <button id="new-prod-btn">Add New Product</button>
                    <%- include('./partials/new-prod-form') %>
                        <main class="items-container">
                            <% prodList.forEach((prod)=> { %>
                                <article class="product">
                                    <div class="text">
                                        <h3>
                                            <%= prod.prodId %>
                                        </h3>
                                        <p>
                                            <%= prod.prodName %>
                                        </p>
                                        <p><img src=<%= prod.imgUrl %> alt="img"></p>
                                        <p>
                                            <%= prod.price %>
                                        </p>
                                    </div>
                                </article>
                        </main>

            </div>


    </body>

</html>